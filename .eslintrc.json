{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "document": true,
        "foo": true,
        "window": true,
        "navigator": true
    },
    "rules": {
       "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "radix": 0,
        "max-len": [1,500,2,{"ignoreComments": true}],
        "no-console": ["error", { "allow": ["warn", "error", "log"] }]
    }   
}